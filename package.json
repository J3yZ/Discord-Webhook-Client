{
  "name": "discord-webhook-client",
  "version": "0.0.1",
  "description": "A Simple wrapper around Discord Webhook endpoints.",
  "main": "dist/index.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "build": "npx tsc",
    "lint": "npx eslint lib/ --ext .ts",
    "test": "npm run build && node __tests__/index.test.js",
    "lint:fix": "npx eslint lib/ --ext .ts --fix",
    "docs": "typedoc --options typedoc.json",
    "postinstall": "npx husky install",
    "prepublishOnly": "npx pinst --disable",
    "postpublish": "npx pinst --enable"
  },
  "prettier": {
    "semi": true,
    "trailingComma": "all",
    "printWidth": 120,
    "tabWidth": 4,
    "endOfLine": "auto"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zaida04/Discord-Webhook-Client.git"
  },
  "keywords": [
    "discord",
    "discord-webhook",
    "discord-webhook-client",
    "discord-webhook-wrapper"
  ],
  "author": "Zaid \"Nico\"",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zaida04/Discord-Webhook-Client/issues"
  },
  "devDependencies": {
    "@types/node": "^14.14.14",
    "@typescript-eslint/eslint-plugin": "^3.9.0",
    "@typescript-eslint/parser": "^3.9.0",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "husky": "^5.0.6",
    "pinst": "^2.1.1",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "typedoc": "^0.19.2",
    "typescript": "^4.0.5"
  },
  "homepage": "https://github.com/zaida04/Discord-Webhook-Client#readme",
  "dependencies": {
    "node-fetch": "^2.6.1"
  }
}
